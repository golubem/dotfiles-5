! .Xdefaults file

!black
*color0: #303030
*color8: #636363

!red
*color1: #be2625
*color9: #c65d57

!green
*color2: #9db68c
*color10: #687e5a

!yellow
*color3: #eaeaae
*color11: #ffe600

!blue
*color4: #596c56
*color12: #8fbc8f

!magenta
*color5: #e79ea9
*color13: #8b6969

!cyan
*color6: #d3d3d3
*color14: #d3d3d3

!white
*color7: #eee9e9
*color15: #e0eee0

urxvt*background: #212121
urxvt*foreground: #c2c2c2

urxvt*transparent: false
urxvt*shading: 5
urxvt*saveLines: 4500
urxvt*scrollBar: false
urxvt*letterSpace: -2
urxvt*cursorBlink: true
urxvt*urlLauncher: firefox
urxvt*perl-ext-common: osc-xterm-clipboard

! fonts settings

urxvt*font: xft:Pragmata Pro:regular:size=10.5, xft:Tewi:regular:size=10

! end
